@model BonVoyage.DAL.Entities.CustomerPreference

@{
    ViewBag.Title = "Delete";
}

<h2>Delete Song</h2>

<h3>Do you really want to delete this Preference?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
             @Html.Label("Preferences:")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PreferencesDetails)
        </dd>
        <dt class="col-sm-2">
            @Html.Label("User:")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="CustomerPreferenceId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back</a>
    </form>
</div>

